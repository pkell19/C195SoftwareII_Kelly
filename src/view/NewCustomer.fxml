<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.effect.DropShadow?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.NewCustomer"
            prefHeight="400.0" prefWidth="1000.0">
    <ButtonBar layoutX="2.0" layoutY="54.0" prefHeight="40.0" prefWidth="992.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#onActionToCustomer" text="Customers" />
            <Button layoutX="472.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionToReport" text="Reports" />
            <Button layoutX="472.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionExit" text="Exit Scheduler" />
            <TextField fx:id="apptSearch" prefWidth="252.0" promptText="Search" />
        </buttons>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
        <effect>
            <DropShadow blurType="TWO_PASS_BOX" />
        </effect>
        <padding>
            <Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
        </padding>
    </ButtonBar>
</AnchorPane>
