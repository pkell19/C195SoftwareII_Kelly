<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AppointmentList">
    <Label alignment="TOP_CENTER" layoutX="397.0" layoutY="14.0" text="Appointment List">
        <font>
          <Font name="Meiryo" size="24.0" />
        </font>
    </Label>
   <HBox layoutX="13.0" layoutY="115.0" prefHeight="25.0" prefWidth="358.0">
      <children>
          <RadioButton mnemonicParsing="false" text="RadioButton" />
          <RadioButton mnemonicParsing="false" text="RadioButton">
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
         </RadioButton>
          <RadioButton mnemonicParsing="false" text="RadioButton" />
      </children>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </HBox>
    <TableView layoutX="13.0" layoutY="144.0" prefHeight="200.0" prefWidth="967.0">
      <columns>
          <TableColumn fx:id="apptId" prefWidth="41.0" text="ID" />
          <TableColumn fx:id="apptTitle" prefWidth="132.0" text="Title" />
          <TableColumn fx:id="apptDescription" prefWidth="182.0" text="Description" />
          <TableColumn fx:id="apptLocation" prefWidth="99.0" text="Location" />
          <TableColumn fx:id="apptContact" prefWidth="86.0" text="Contact" />
          <TableColumn fx:id="apptType" prefWidth="74.0" text="Type" />
          <TableColumn fx:id="apptDate" prefWidth="61.0" text="Date" />
          <TableColumn fx:id="apptStartTime" prefWidth="78.0" text="Start Time" />
          <TableColumn fx:id="apptEndTime" prefWidth="70.0" text="End Time" />
          <TableColumn fx:id="apptCustomerId" prefWidth="86.0" text="Customer ID" />
          <TableColumn fx:id="apptUserId" prefWidth="57.0" text="User ID" />
      </columns>
    </TableView>
   <HBox layoutX="13.0" layoutY="350.0" prefHeight="25.0" prefWidth="443.0">
      <children>
          <Button fx:id="deleteAppt" mnemonicParsing="false" text="Delete Appointment">
            <HBox.margin>
               <Insets right="25.0" />
            </HBox.margin>
         </Button>
          <Button fx:id="updateAppt" mnemonicParsing="false" text="Modify Appointment">
            <HBox.margin>
               <Insets right="25.0" />
            </HBox.margin>
         </Button>
          <Button fx:id="newAppt" alignment="CENTER" mnemonicParsing="false" text="New Appointment">
            <HBox.margin>
               <Insets right="25.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>
   <ButtonBar layoutX="13.0" layoutY="54.0" prefHeight="40.0" prefWidth="967.0">
     <buttons>
       <Button mnemonicParsing="false" onAction="#onActionToCustomer" text="Customers" />
         <Button layoutX="472.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionToReport" text="Reports" />
         <Button layoutX="472.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionExit" text="Exit Scheduler" />
         <TextArea fx:id="apptSearch" prefHeight="40.0" prefWidth="252.0" promptText="Search" />
     </buttons>
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <effect>
         <DropShadow blurType="TWO_PASS_BOX" />
      </effect>
   </ButtonBar>
</AnchorPane>
